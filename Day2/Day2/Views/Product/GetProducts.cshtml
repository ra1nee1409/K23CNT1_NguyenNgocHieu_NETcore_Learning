@{
    ViewData["Title"] = "GetProducts";
}

<h2>Dữ liệu lấy ra từ ViewBag</h2>
<p>Mã sản phẩm: @ViewBag.product.ProductId</p>
<p>Tên sản phẩm: @ViewBag.product.ProductName</p>
<p>Năm sản xuất: @ViewBag.product.YearRelease</p>
<p>Giá: @ViewBag.product.Price</p>

<h2>Dữ liệu lấy ra từ ViewData</h2>
@{
    var product = ViewData["productVD"] as Day2.Models.Product;
}
<p>Mã sản phẩm: @product.ProductId</p>
<p>Tên sản phẩm: @product.ProductName</p>
<p>Năm sản xuất: @product.YearRelease</p>
<p>Giá: @product.Price</p>
